#!/bin/sh
# usage: GraphViewer
#  -d,--decorateedges         decorate edges according to the number of
#                             paths [false]
#  -g,--graph <arg>           name of graph
#  -h,--highlightpath <arg>   path to highlight

graph=$1

java -Xmx160g -XX:+UseParallelGC -Xms2g -XX:SoftRefLRUPolicyMSPerMB=1 -XX:MaxHeapFreeRatio=99 -cp "build/install/fr/lib/*" org.ncgr.pangenomics.GraphViewer -g $graph $2 $3 $4 $5 $6
